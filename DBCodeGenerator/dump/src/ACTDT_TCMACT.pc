//updates a single entry from DB
char* ACTDT_TCMACT::updateDB()
{
	//
	//copy the host variables (k_...) to Oracle Host Keys (k_ora_..)
	//
		copyKeyToHost();
	//copy the attributes ( ...) to Oracle Hosts( ora_..)
	//
		copyAttributeToOracle();
	//
	// Do UPDATE
	//
	EXEC SQL UPDATE ACTDT_TCMACT
	SET 
		datee               =:ora_datee,
		timee               =:ora_timee,
		samp_no             =:ora_samp_no,
		linsts              =:ora_linsts:ora_linstsZi,
		rmd                 =:ora_rmd:ora_rmdZi,
		trno                =:ora_trno:ora_trnoZi,
		dia                 =:ora_dia:ora_diaZi,
		acd                 =:ora_acd:ora_acdZi,
		mrh                 =:ora_mrh:ora_mrhZi,
		dstd                =:ora_dstd:ora_dstdZi,
