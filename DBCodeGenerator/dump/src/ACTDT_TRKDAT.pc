//Executes the SQL count(key) for a where clause
char* ACTDT_TRKDAT::countDB(const char* where,long* count)
{
	*(count) = -1L;
//
// Create dynamic statement. 
//
	strncpy((char*)dynstr.arr,"SELECT count(samp_no)", sizeof(dynstr.arr));
	strcat((char*)dynstr.arr," FROM ACTDT_TRKDAT");
	if ( where != (char*)NULL )
	{
		strcat( (char*)dynstr.arr," ");
		 strcat( (char*)dynstr.arr, where );
	}
	dynstr.len = strlen( (char*) dynstr.arr);
	EXEC SQL PREPARE c_ACTDT_TRKDAT FROM :dynstr;
	
	if (sqlca.sqlcode == 0 )
	{
		EXEC SQL DECLARE ct_ACTDT_TRKDAT CURSOR FOR c_ACTDT_TRKDAT;
		if (sqlca.sqlcode == 0 )
		{
			EXEC SQL OPEN ct_ACTDT_TRKDAT;
			if (sqlca.sqlcode == 0 )			{
				EXEC SQL FETCH ct_ACTDT_TRKDAT INTO :rowCnt;
				if (sqlca.sqlcode == 0 )
				*(count) = rowCnt;
			}
			EXEC SQL CLOSE ct_ACTDT_TRKDAT;
		}
	}

	//
	// ORACLE Error 
	//
	oracleSqlCode = sqlca.sqlcode;
	oracleRowsProcessed = sqlca.sqlerrd[2];
	if ( sqlca.sqlcode != 0 )
	{
		sqlca.sqlerrm.sqlerrmc[sqlca.sqlerrm.sqlerrml] = '\0';
		logOraMessageInfo();
		pCatch->catchError(sqlca.sqlcode);
		return (char*)sqlca.sqlerrm.sqlerrmc;
	}
	return ( NULL );
}

